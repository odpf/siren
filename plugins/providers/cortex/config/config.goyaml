templates:
  - 'helper.tmpl'
global:
  pagerduty_url: https://events.pagerduty.com/v2/enqueue
  resolve_timeout: 5m
  slack_api_url: https://slack.com/api/chat.postMessage
receivers:
  - name: default
    webhook_configs:
      - url: '[[.WebhookURL]]'
route:
  receiver: default
  group_by:
    - alertname
    - severity
    - owner
    - service_name
    - time_stamp
    - identifier
  group_wait: [[.GroupWaitDuration]]
  group_interval: 30m
  repeat_interval: 4h